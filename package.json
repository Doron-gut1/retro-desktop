{
  "name": "retro-desktop",
  "version": "1.0.0",
  "description": "Desktop application for retroactive calculations",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dev": "concurrently \"npm run build:electron\" \"npm run start:electron\" \"npm run start:react\"",
    "start:electron": "wait-on tcp:5173 && electron .",
    "start:react": "vite",
    "build": "tsc && vite build",
    "build:electron": "tsc electron/main.ts --outDir dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doron-gut1/retro-desktop.git"
  },
  "author": "Doron Gut",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "lucide-react": "^0.263.1",
    "mssql": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.0.0",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.0.0",
    "@electron-forge/maker-deb": "^7.0.0",
    "@electron-forge/maker-rpm": "^7.0.0",
    "@electron-forge/maker-squirrel": "^7.0.0",
    "@electron-forge/maker-zip": "^7.0.0",
    "@types/mssql": "^9.1.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "concurrently": "^8.0.0",
    "electron": "^28.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "postcss": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}