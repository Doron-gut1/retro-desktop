{
  "name": "retro-desktop",
  "version": "1.0.0",
  "description": "Desktop application for retroactive calculations",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "preview": "vite preview",
    "start": "electron dist/electron/main.js",
    "dev:electron": "concurrently \"npm run dev\" \"npm run build:electron -- --watch\" \"wait-on http://localhost:5173 && electron .\"",
    "build:all": "npm run build && npm run build:electron",
    "start:prod": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doron-gut1/retro-desktop.git"
  },
  "author": "Doron Gut",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "lucide-react": "^0.263.1",
    "mssql": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/mssql": "^9.1.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0", 
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "postcss": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  }
}